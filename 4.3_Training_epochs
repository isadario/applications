   "source": [
    "#plotto i pallini per ogni epoca con slider \n",
    "fig = px.scatter_3d(mvm_data, #il dataset risultante \n",
    "                    x='x_point',\n",
    "                    y='y_point',\n",
    "                    z='z_point',\n",
    "                    animation_frame='epoch', #per mettere lo slider \n",
    "                    size_max=18,\n",
    "                    hover_name ='epoch', #cosìquando clicco sopra il pallino mi dice a quale epoca è associato \n",
    "                    color = 'MCC',\n",
    "                    range_x=[0,1], \n",
    "                    range_y=[0,1],\n",
    "                    range_z=[0,1],\n",
    "                   range_color=(-1,1)) #per fissare il range della metrica sennò non mi cambia colore \n",
    "\n",
    "\n",
    "fig.update_traces(marker=dict(size=12,\n",
    "                              line=dict(width=2,\n",
    "                                        color='black')),\n",
    "                  selector=dict(mode='circle-open'))\n",
    "\n",
    "\n",
    "#then add the results of each epoch\n",
    "fig.add_trace(go.Figure(data=[\n",
    "    go.Mesh3d(\n",
    "        x=[0, 0, 0, 1], #verticalmente ho definito O, C, B, A\n",
    "        y=[0, 0, 1, 0],\n",
    "        z=[0, 1, 0, 0],\n",
    "        \n",
    "        color ='lightgrey',\n",
    "        \n",
    "        # Intensity of each vertex, which will be interpolated and color-coded\n",
    "        #intensity=[0, 0.33, 0.66, 1],\n",
    "        \n",
    "        opacity = 0.2, #for transparency\n",
    "        \n",
    "        # i, j and k sono i vertici dei triangoli\n",
    "        # here we represent the 4 triangles of the tetrahedron surface\n",
    "        i=[0, 0, 0, 1],\n",
    "        j=[1, 2, 3, 2],\n",
    "        k=[2, 3, 1, 3],\n",
    "        name='y',\n",
    "        showscale=True\n",
    "    )\n",
    "]).data[0])"
