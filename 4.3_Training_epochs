   "source": [
    "#train the model\n",
    "hist = model.fit(x_train, #input data\n",
    "                 y_train, #labels\n",
    "                 validation_data=(x_test, y_test), \n",
    "                 epochs=100, #number of iterations\n",
    "                 batch_size=10, #divides data into batches of equal size \n",
    "                                #since I have few records I choose small size of 10\n",
    "                                #but in general is just to avoid overload in memory\n",
    "                 callbacks = [GetEpochOutput(x_test)]\n",
    "                ) \n",
    "\n",
    "\n",
    "\n",
    "#returns the history of model training\n",
    "#consists of model accuracy and losses after each epoch"
   ]
