    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "2022-03-10 01:50:03.690477: I tensorflow/core/platform/cpu_feature_guard.cc:151] This TensorFlow binary is optimized with oneAPI Deep Neural Network Library (oneDNN) to use the following CPU instructions in performance-critical operations:  AVX2 FMA\n",
      "To enable them in other operations, rebuild TensorFlow with the appropriate compiler flags.\n"
     ]
    }
   ],
   "source": [
    "#create a neural network for binary classification\n",
    "#import keras\n",
    "from keras.models import Sequential\n",
    "from keras.layers import Dense\n",
    "\n",
    "#instatiate the model \n",
    "model = Sequential()\n",
    "#Sequential() -> specifies to keras that we are creating model sequentially and \n",
    "#the output of each layer we add is input to the next layer we specify\n",
    "\n",
    "\n",
    "\n",
    "#input layer with 128 neuroni \n",
    "model.add(Dense(128, \n",
    "                activation='relu', #activation function: Rectified Linear Unit (RELU)\n",
    "                input_dim=48))     #\n",
    "#model.add -> is used to add a layer to our neural network\n",
    "#Dense -> specifies the fully connected layer (so the connections between the layers)\n",
    "\n",
    "\n",
    "#hidden layer 1\n",
    "model.add(Dense(24, \n",
    "                    activation='relu'))\n",
    "                    #no need to specify input dimension as we have defined \n",
    "                    #the model to be sequential so keras will automatically \n",
    "                    #consider input dimension to be same as the output of \n",
    "                    #last layer i.e 48.\n",
    "\n",
    "#hidden layer 2\n",
    "model.add(Dense(32, activation='relu')) #another hidden layer (intermediate point)\n",
    "\n",
    "\n",
    "#output layer \n",
    "model.add(Dense(1, \n",
    "                activation='sigmoid')) #the output layer takes different activation functions \n",
    "                                       #for binary classification should be 'sigmoid'\n",
    "\n",
    "#specify the loss function and the optimizer\n",
    "model.compile(loss='binary_crossentropy', \n",
    "              optimizer='adam', \n",
    "              metrics=['accuracy']) #the way we want to judge the NN\n",
    "\n",
    "model.summary()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "ba5588f7",
   "metadata": {},
   "outputs": [],
   "source": [
    "from keras import layers\n",
    "from keras import models\n",
    "#from tensorflow.keras import optimizers\n",
    "#import tensorflow\n",
    "from keras import losses\n",
    "from keras import metrics as metricske\n",
    "\n",
    "from keras.callbacks import Callback #callback vengono chiamate quando qualcosa succede\n",
    "#ci sono tante callback (intervengono alla conclusione di ogni iter), io le uso per andare a operare su ogni epoca \n",
    "from keras import backend as K\n",
    "\n",
    "#inizializzo lista in cui ad ogni epoca aggiungerò\n",
    "lst_points = np.empty((0,3), float)\n",
    "\n",
    "\n",
    "N_test = len(x_test)\n",
    "#TODO:meglio usare numpy probabilmente per poi trasformare in dataframe\n",
    "\n",
    "                                #creo classe GetEpochOutput che \n",
    "class GetEpochOutput(Callback): #eredita la classe callback di Keras (per poter modificare le callback)       \n",
    "    def __init__(self, x_test): #costruttore - gli passerò come oggetto X_test e lst_points\n",
    "        #self.lst = lst           #y_test, lst -non mi servono più perchè si recupera la variabile globale\n",
    "        self.x_test = x_test\n",
    "        #self.y_test = y_test \n",
    "\n",
    "    def on_epoch_end(self, epoch, logs={}):\n",
    "        global lst_points #need to define the global variable otherwise I kept on getting the empty lst_points defined outside \n",
    "                          #by defining the global variable instead of making everytime a copy I make it update the original lst_points \n",
    "            \n",
    "        prediction = self.model.predict(self.x_test).round()\n",
    "        #print(prediction)\n",
    "        cm = confusion_matrix(prediction, y_test)\n",
    "        x_epoch = cm[0][0]/N_test #TP/N con N size del test\n",
    "        y_epoch = cm[1][1]/N_test #TN/N\n",
    "        z_epoch = cm[0][1]/N_test #FP/N\n",
    "        \n",
    "        lst_points = np.append(lst_points, np.array([[x_epoch,y_epoch,z_epoch]]), axis=0)\n",
    "        #self.lst.append([x_epoch,y_epoch,z_epoch])  #finchè usavo lista (poi numpy)        \n",
    "        "
   ]
  },
