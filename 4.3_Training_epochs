   ],
   "source": [
    "#aggiungo MCC al dataframe \n",
    "\n",
    "#bad code ma è per fare veloce a gestire l'errore per l'argomento della radice quadrata \n",
    "#quando l'mcc non sarebbe definito perchè una delle righe o delle colonne è zero\n",
    "\n",
    "MCC_val = []\n",
    "\n",
    "for index, row in mvm_data.iterrows():\n",
    "    arg_rad = (row['x_point']+row['z_point'])*(row['x_point']+(1-row['z_point']-row['x_point']-row['y_point']))*(row['y_point']+row['z_point'])*(row['y_point']+(1-row['z_point']-row['x_point']-row['y_point']))\n",
    "    if arg_rad <=0:  \n",
    "        MCC_val.append(0) #MCC is undefined when a whole row or column is zero \n",
    "                          #ma il limite tende a 0 quindi posso mettere 0  \n",
    "                          #è discusso anche nel paper https://bmcgenomics.biomedcentral.com/articles/10.1186/s12864-019-6413-7\n",
    "    else: \n",
    "        MCC_val.append((row['x_point']*row['y_point'] \n",
    "                 - row['z_point']*(1-row['z_point']-row['x_point']-row['y_point'])) \n",
    "    /math.sqrt((row['x_point']+row['z_point'])\n",
    "               *(row['x_point']+(1-row['z_point']-row['x_point']-row['y_point']))\n",
    "               *(row['y_point']+row['z_point'])\n",
    "               *(row['y_point'] +(1-row['z_point']-row['x_point']-row['y_point']))))\n",
    "        \n",
    "mvm_data['MCC'] = MCC_val\n",
    "mvm_data"
   ]
  },
